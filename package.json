{
  "name": "gen-redux-webpack-plugin",
  "version": "1.3.3",
  "description": "Webpack plugin to generate redux actions and reducers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gen-redux": "dist/bin/gen-redux.js"
  },
  "scripts": {
    "test": "jest --watchAll --collectCoverage",
    "test:ci": "jest --collectCoverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "typecheck": "tsc -p ./tsconfig.check.json",
    "gen-version": "echo \"{\n    \\\"version\\\": \\\"$npm_package_version\\\"\n}\" > src/version.json",
    "build": "npm run gen-version && tsc -p .",
    "prepare": "npm run build",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajscholl/gen-redux-webpack-plugin"
  },
  "keywords": [
    "webpack",
    "plugin",
    "redux",
    "actions",
    "reducer",
    "generator"
  ],
  "author": "Anselm Jonas Scholl",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^28.1.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "yaml": "^2.1.1",
    "yup": "^0.32.11"
  },
  "engines": {
    "node": "^12.13.0 || ^14.15.0 || ^16.0.0 || >=17.0.0"
  }
}
