{
  "name": "gen-redux-webpack-plugin",
  "version": "1.3.5",
  "description": "Webpack plugin to generate redux actions and reducers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gen-redux": "dist/bin/gen-redux.js"
  },
  "scripts": {
    "test": "jest --watchAll --collectCoverage",
    "test:ci": "jest --collectCoverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "typecheck": "tsc -p ./tsconfig.check.json",
    "gen-version": "echo \"{\n    \\\"version\\\": \\\"$npm_package_version\\\"\n}\" > src/version.json",
    "build": "npm run gen-version && tsc -p .",
    "prepare": "npm run build",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajscholl/gen-redux-webpack-plugin"
  },
  "keywords": [
    "webpack",
    "plugin",
    "redux",
    "actions",
    "reducer",
    "generator"
  ],
  "author": "Anselm Jonas Scholl",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.23",
    "@types/react": "^18.2.61",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "yaml": "^2.4.0",
    "yup": "^0.32.11"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=21.0.0"
  }
}
