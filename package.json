{
  "name": "gen-redux-webpack-plugin",
  "version": "1.3.4",
  "description": "Webpack plugin to generate redux actions and reducers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gen-redux": "dist/bin/gen-redux.js"
  },
  "scripts": {
    "test": "jest --watchAll --collectCoverage",
    "test:ci": "jest --collectCoverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "typecheck": "tsc -p ./tsconfig.check.json",
    "gen-version": "echo \"{\n    \\\"version\\\": \\\"$npm_package_version\\\"\n}\" > src/version.json",
    "build": "npm run gen-version && tsc -p .",
    "prepare": "npm run build",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajscholl/gen-redux-webpack-plugin"
  },
  "keywords": [
    "webpack",
    "plugin",
    "redux",
    "actions",
    "reducer",
    "generator"
  ],
  "author": "Anselm Jonas Scholl",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "yaml": "^2.2.1",
    "yup": "^0.32.11"
  },
  "engines": {
    "node": "^14.15.0 || ^16.0.0 || ^18.0.0 || >=19.0.0"
  }
}
